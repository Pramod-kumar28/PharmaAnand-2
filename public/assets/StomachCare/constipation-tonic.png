            enableOfflineStoreRadio.checked = false;
            enableOnlineStoreRadio.checked = false;

            if (onlineCalendar.style && offlineCalendar.style) {
                onlineCalendar.style['display'] = 'block';
                offlineCalendar.style['display'] = 'block';
            }
